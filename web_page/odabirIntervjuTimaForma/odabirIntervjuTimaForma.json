        "failedResponseValue" : ""
      },
      "exposed" : false
    },
    "clanoviOglasJezici" : {
      "type" : "expression",
      "value" : [
        "let oglasJezici = $data.prijavaNaOglas_oglasZaPosao_straniJezici;",
        "let sviJeziciId = [];",
        "for (let i = 0; i < oglasJezici.length; i++) {",
        "    let jezik = oglasJezici[i];",
        "    if (jezik !== null) {",
        "        sviJeziciId.push(jezik.persistenceId_string);",
        "    }",
        "}",
        "let clanoviJezici = $data.clanoviStrucniJezici;",
        "let odabraniClanoviIds = [];",
        "for (let i = 0; i < clanoviJezici.length; i++) {",
        "    let jezici = clanoviJezici[i].value;",
        "    for (let j = 0; j < sviJeziciId.length; j++) {",
        "        if (jezici.includes(sviJeziciId[j])) {",
        "            odabraniClanoviIds.push(clanoviJezici[i].userId);",
        "            break;",
        "        }",
        "    }",
        "}",
        "// {'id': \"1\", 'fullName': \"\"}",
        "let sviClanovi = $data.clanoviJez;",
        "let odabraniClanovi = [];",
        "for (let i = 0; i < odabraniClanoviIds.length; i++) {",
        "    for (let j = 0; j < sviClanovi.length; j++) {",
        "        if (odabraniClanoviIds[i] === sviClanovi[j].id) {",
        "            odabraniClanovi.push(",
        "                {'id': sviClanovi[j].id, ",
        "                'fullName': sviClanovi[j].firstname + ' ' ",
        "                            + sviClanovi[j].lastname});",
        "        }",
        "    }",
        "}",
        "return odabraniClanovi;"
      ],
      "exposed" : false
    },
