        "\t$data.rezultujuciZahtev.ponudaDobavljaca = $data.rezultujuciZahtev_ponudaDobavljaca;",
        "\treturn {",
        "\t\t//map rezultujuciZahtev variable to expected task contract input",
        "\t\trezultujuciZahtevInput: {",
        "\t\t\tponudaDobavljaca: $data.rezultujuciZahtev.ponudaDobavljaca.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
