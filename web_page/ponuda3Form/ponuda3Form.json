    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.zahteviZaPonude ){",
        "\treturn {",
        "\t\t//map zahteviZaPonude variable to expected task contract input",
        "\t\tzahteviZaPonudeInput: $data.zahteviZaPonude.map( it => ({",
        "\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\tnazivOpreme: it.nazivOpreme !== undefined ? it.nazivOpreme : null,",
        "\t\t\tnedostajucaKolicina: it.nedostajucaKolicina !== undefined ? it.nedostajucaKolicina : null,",
        "\t\t\tcena: it.cena !== undefined ? it.cena : null",
        "\t\t}))",
        "\t}",
        "}"
      ],
